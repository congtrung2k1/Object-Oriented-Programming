package rectangle;

class Rectangle {
	double w, h;
	
	Rectangle(double width, double height){
		if (width < 0) {
			System.out.println("Width Invalid.");
			this.w = -1 * width;
		} else 
			this.w = width;
		
		if (height < 0) {
			System.out.println("Height Invalid.");
			this.h = -1 * height;
		} else 
			this.h = height; 
	}
	
	static void display(int w, int h) {
		for (int i = 0; i < h; ++i) {
			for (int j = 0; j < w; ++j)
				System.out.println('*');
			System.out.println('\n');
		}
		System.out.println('\n');
	}
	
	public class TestRectangle {
		public void main(String[] args) {
			Rectangle rect_1 = new Rectangle(10.0, 5.0);
			Rectangle rect_2 = new Rectangle(10.0, 5.0);
			Rectangle rect_3 = new Rectangle(10.0, 5.0);
			Rectangle rect_4 = new Rectangle(10.0, 5.0);
			Rectangle rect_5 = new Rectangle(10.0, 5.0);
		}
	}
}
